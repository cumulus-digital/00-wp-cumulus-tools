{
  "_#": "Wordpress features support",
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "cumulus-gutenberg/donut-graph",
  "title": "Donut Graph",
  "description": "A simple donut graph block",
  "keywords": [
    "cumulus",
    "cmls",
    "graph",
    "chart",
    "donut"
  ],
  "icon": "star-filled",
  "category": "cmls",
  "editorScript": "file:./assets/block.js",
  "style": "file:./assets/block.css",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": null
    },
    "width": {
      "type": "string",
      "default": "100px"
    },
    "percent": {
      "type": "number",
      "default": 35
    },
    "rotation": {
      "type": "number",
      "default": 0
    },
    "strokeWidth": {
      "type": "number",
      "default": 20
    },
    "colorActive": {
      "type": "string",
      "default": "#00588d"
    },
    "colorInactive": {
      "type": "string",
      "default": "#eeeeee"
    },
    "showPercentLabel": {
      "type": "boolean",
      "default": true
    },
    "percentLabelColor": {
      "type": "string",
      "default": null
    },
    "percentLabelFontSize": {
      "type": "string",
      "default": "1em"
    },
    "percentLabelFontWeight": {
      "type": "string",
      "default": null
    },
    "percentLabelFontStyle": {
      "type": "string",
      "default": null
    }
  },
  "supports": {
    "_#": "Allow customizing border",
    "align": true,
    "alignWide": false,
    "className": true,
    "color": {
      "background": false,
      "__experimentalDuotone": false,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": false,
    "defaultStylePicker": true,
    "html": false,
    "inserter": true,
    "multiple": true,
    "reusable": true,
    "lock": true,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "align": false,
      "fontSize": true,
      "lineHeight": false,
      "fontWeight": true,
      "fontStyle": true,
      "textTransform": false,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": false
    },
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false,
      "defaultControls": {
        "color": false,
        "radius": false,
        "style": false,
        "width": false
      }
    },
    "__experimentalBorder": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false,
      "__experimentalDefaultControls": {
        "color": false,
        "radius": false,
        "style": false,
        "width": false
      }
    }
  }
}